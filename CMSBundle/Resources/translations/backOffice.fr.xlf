<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>

<!-- GENERAL -->

            <group id="general">
                <trans-unit id="loading">
                    <source>loading</source>
                    <target>Chargement ...</target>
                </trans-unit>
            </group>

<!-- TOP MENU -->

            <group id="topMenu">
                <trans-unit id="site">
                    <source>site</source>
                    <target>Site :</target>
                </trans-unit>
                <trans-unit id="collapse_menu">
                    <source>collapse_menu</source>
                    <target>Replier le menu</target>
                </trans-unit>
                <trans-unit id="sign_out">
                    <source>sign_out</source>
                    <target>Se déconnecter</target>
                </trans-unit>
                <trans-unit id="improve_security_closing_browser">
                    <source>improve_security_closing_browser</source>
                    <target>Vous pouvez améliorer votre sécurité après la déconnexion en fermant votre navigateur</target>
                </trans-unit>
                <trans-unit id="fullscreen">
                    <source>fullscreen</source>
                    <target>Plein écran</target>
                </trans-unit>
                <trans-unit id="language_flag">
                    <source>%language%_flag</source>
                    <target>drapeau language</target>
                </trans-unit>
            </group>

<!-- LEFT MENU -->

            <group id="leftMenu">
                
                <group id="editorial">
                    <trans-unit id="edito.editorial">
                        <source>edito.editorial</source>
                        <target>Editorial</target>
                    </trans-unit>
                    <trans-unit id="edito.nodes">
                        <source>edito.nodes</source>
                        <target>Pages</target>
                    </trans-unit>
                    <trans-unit id="edito.templates">
                        <source>edito.templates</source>
                        <target>Gabarits</target>
                    </trans-unit>
                    <trans-unit id="edito.contents">
                        <source>edito.contents</source>
                        <target>Contenus</target>
                    </trans-unit>
                </group>
                
                <group id="administration">
                    <trans-unit id="admin.administration">
                        <source>admin.administration</source>
                        <target>Administration</target>
                    </trans-unit>
                    <trans-unit id="admin.sites">
                        <source>admin.sites</source>
                        <target>Sites</target>
                    </trans-unit>
                    <trans-unit id="admin.contentTypes">
                        <source>admin.contentTypes</source>
                        <target>Types de contenus</target>
                    </trans-unit>
                    <trans-unit id="admin.routingCache">
                        <source>admin.routingCache</source>
                        <target>Cache de routage</target>
                    </trans-unit>
                    <trans-unit id="admin.routingCache.dialog.title">
                        <source>admin.routingCache.dialog.title</source>
                        <target>Cache de routage</target>
                    </trans-unit>
                    <trans-unit id="admin.routingCache.dialog.message">
                        <source>admin.routingCache.dialog.message</source>
                        <target>Vous êtes sur le point de supprimer le cache de routage. Souhaitez-vous continuer ?</target>
                    </trans-unit>
                    <trans-unit id="admin.routingCache.ok">
                        <source>admin.routingCache.ok</source>
                        <target>Le cache de routage a été purgé avec succès.</target>
                    </trans-unit>
                </group>

<!-- TABLE VIEW -->

                <group id="tableView">
                    <trans-unit id="tableView.deleteDialog.title">
                        <source>tableView.deleteDialog.title</source>
                        <target>Suppression du document</target>
                    </trans-unit>
                    <trans-unit id="tableView.deleteDialog.message">
                        <source>tableView.deleteDialog.message</source>
                        <target>Vous êtes sur le point de supprimer le document. Souhaitez-vous continuer ?</target>
                    </trans-unit>
                    <trans-unit id="tableView.filterLabel">
                        <source>filter%label%</source>
                        <target>Filtre label</target>
                    </trans-unit>
                    <trans-unit id="tableView.edition">
                        <source>tableView.edition</source>
                        <target>Edition</target>
                    </trans-unit>
                    <trans-unit id="tableView.list">
                        <source>tableView.list</source>
                        <target>Liste</target>
                    </trans-unit>
                    <trans-unit id="tableView.modify">
                        <source>tableView.modify</source>
                        <target>Modifier</target>
                    </trans-unit>
                    <trans-unit id="tableView.delete">
                        <source>tableView.delete</source>
                        <target>Supprimer</target>
                    </trans-unit>
                    <trans-unit id="tableView.add">
                        <source>tableView.add</source>
                        <target>Ajouter</target>
                    </trans-unit>
                    <trans-unit id="tableView.save">
                        <source>tableView.save</source>
                        <target>Enregistrer</target>
                    </trans-unit>
                    <trans-unit id="tableView.back">
                        <source>tableView.back</source>
                        <target>Retour</target>
                    </trans-unit>
                </group>
                
            </group>

<!-- CONTENT TYPES -->

            <group id="contentTypes">
                
                <trans-unit id="contentTypes.mainTitle">
                    <source>contentTypes.mainTitle</source>
                    <target>Types de contenu</target>
                </trans-unit>
                
                <group id="contentTypes.list">
                    <trans-unit id="contentTypes.list.identifier">
                        <source>contentTypes.list.identifier</source>
                        <target>Identifiant</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.list.name">
                        <source>contentTypes.list.name</source>
                        <target>Nom</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.list.version">
                        <source>contentTypes.list.version</source>
                        <target>Version</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.list.status">
                        <source>contentTypes.list.status</source>
                        <target>Statut</target>
                    </trans-unit>
                </group>
                
                <group id="contentTypes.form">
                    <trans-unit id="contentTypes.form.identifier">
                        <source>contentTypes.form.identifier</source>
                        <target>Identifiant technique</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.label">
                        <source>contentTypes.form.label</source>
                        <target>Intitulé du type de contenu</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.version">
                        <source>contentTypes.form.version</source>
                        <target>Version</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.status">
                        <source>contentTypes.form.status</source>
                        <target>Statut</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.newVersion">
                        <source>contentTypes.form.newVersion</source>
                        <target>Créer une nouvelle version</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.versX">
                        <source>contentTypes.form.version%versionNumber%</source>
                        <target>Version versionNumber</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.typeDefinition">
                        <source>contentTypes.form.typeDefinition</source>
                        <target>Définition du type de contenu</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.fieldX">
                        <source>contentTypes.form.field%fieldId%</source>
                        <target>Champ "fieldName"</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.newField">
                        <source>contentTypes.form.newField</source>
                        <target>Nouveau champ</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.fieldLabel">
                        <source>contentTypes.form.fieldLabel</source>
                        <target>Intitulé du champ</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.defaultValue">
                        <source>contentTypes.form.defaultValue</source>
                        <target>Valeur par défaut</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.indexable">
                        <source>contentTypes.form.indexable</source>
                        <target>Indexable</target>
                    </trans-unit>
                    <trans-unit id="contentTypes.form.removeField">
                        <source>contentTypes.form.removeField</source>
                        <target>Supprimer le champ</target>
                    </trans-unit>
                </group>
                
            </group>

<!-- CONTENTS -->

            <group id="contents">
                
                <trans-unit id="contents.mainTitleX">
                    <source>contents.mainTitle%contentType%</source>
                    <target>Contenus de type contentType</target>
                </trans-unit>
                
                <group id="contents.list">
                    <trans-unit id="contents.list.contentId">
                        <source>contents.list.contentId</source>
                        <target>Identifiant</target>
                    </trans-unit>
                    <trans-unit id="contents.list.label">
                        <source>contents.list.label</source>
                        <target>Intitulé</target>
                    </trans-unit>
                    <trans-unit id="contents.list.language">
                        <source>contents.list.language</source>
                        <target>Langue</target>
                    </trans-unit>
                    <trans-unit id="contents.list.version">
                        <source>contents.list.version</source>
                        <target>Version</target>
                    </trans-unit>
                </group>
                
                <group id="contents.form">
                    <trans-unit id="contents.form.name">
                        <source>contents.form.name</source>
                        <target>Intitulé du contenu</target>
                    </trans-unit>
                    <trans-unit id="contents.form.version">
                        <source>contents.form.version</source>
                        <target>Version</target>
                    </trans-unit>
                    <trans-unit id="contents.form.newVersion">
                        <source>contents.form.newVersion</source>
                        <target>Créer une nouvelle version</target>
                    </trans-unit>
                    <trans-unit id="contents.form.versX">
                        <source>contents.form.version%versionNumber%</source>
                        <target>Version versionNumber</target>
                    </trans-unit>
                </group>
                
            </group>
            
        </body>
