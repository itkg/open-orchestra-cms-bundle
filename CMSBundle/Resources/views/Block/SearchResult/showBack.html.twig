{#{ dump(resultset.numfound) }#}

{# if they are 0 result #}
{% if resultset.numfound is not defined %}
    {% if resultset is empty %}
        {{ 'Sorry no results for'|trans }} "{{ data }}".
    {% else %}
        {# Browse the result list and call this template #}
        {% for result in resultset %}
            {#{ dump(result) }#}
            {% include "PHPOrchestraCMSBundle:Block/SearchResult:showBack.html.twig" with {"data": data, "resultset": result, "nodeId": nodeId} %}
        {% endfor %}
    {% endif %}
{% else %}
    <div id="searchid">
        {#{ dump(resultset) }#}

        {% if resultset.spellcheck is not empty %}
            {# Call spellCheck template #}
            {% include "PHPOrchestraCMSBundle:Block/SearchResult:spellcheck.html.twig" %}
        {% endif %}

        {# This template display list of facet #}
        {% if resultset.facetSet is not empty %}
            {% include "PHPOrchestraCMSBundle:Block/SearchResult:facet.html.twig" %}
        {% endif %}

        {% if resultset is not empty %}
            {# This template display list of documents in the result#}
            {% include "PHPOrchestraCMSBundle:Block/SearchResult:showDocs.html.twig" %}
        {% else %}
            {{ 'Sorry no results for'|trans }} "{{ data }}".
        {% endif %}

    </div>
{% endif %}
