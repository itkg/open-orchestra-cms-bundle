{% block oo_registration_user_submit_widget %}
    {% spaceless %}
        {{ block('form_submit_widget') }}
        {% set label = 'open_orchestra_backoffice.form.submit_continue' %}
        {% set id = 'submit_' ~ form.vars.name %}
        {% set attr = {'class': 'submit-continue-form'} %}
        {% set button_class= 'success' %}
        {{ block('button_widget') }}
    {% endspaceless %}
{% endblock oo_registration_user_submit_widget %}
{% block oo_member_list_widget %}
	{% set collection = form|first %}
    {% spaceless %}
        <div class="group-users-list">
            <table class="table table-striped">
            	<thead>
                    <tr>
                        <th>{{ 'open_orchestra_group.form.group.user_name'|trans }}</th>
                        <th>{{ 'open_orchestra_group.form.group.user_email'|trans }}</th>
                        <th>{{ 'open_orchestra_group.form.group.user_delete'|trans }}</th>
                    </tr>
                </thead>
                <tbody>
            {% for child in collection %}
                {{ form_widget(child) }}
            {% endfor %}
                </tbody>
            </table>
        </div>
    {% endspaceless %}
{% endblock oo_member_list_widget %}

{% block oo_member_element_widget %}
    <tr>
        <td><div class="hide">{{ form_widget(form) }}</div><a href="#" data-id="{{ form.vars.parameters['id'] }}" class="member-link">{{ form.vars.parameters['firstName'] }}&nbsp;{{ form.vars.parameters['lastName'] }}</a></td>
        <td>{{ form.vars.parameters['email'] }}</td>
        <td>
            <input id="{{ form.vars.id }}_checkbox" class="delete-checkbox group-user" type="checkbox"><label for="{{ form.vars.id }}_checkbox"></label>
        </td>
    </tr>
{% endblock oo_member_element_widget %}
