<% if (deleted != true) { %>
    <% if (typeof links._self_duplicate !== 'undefined' ) { %>
    <a href="#" class="btn btn-default btn-xs ajax-duplicate" title="{{ 'open_orchestra_backoffice.form.duplicate'|trans({}, 'messages', language) }}">
        <span class="glyphicon glyphicon-duplicate" aria-hidden="true"></span>
    </a>
    <% } %>
    <% if (typeof links._self_form !== 'undefined') { %>
        <a href="#" class="btn btn-warning btn-xs ajax-edit" title="{{ 'open_orchestra_backoffice.form.edit'|trans({}, 'messages', language) }}">
    <% } else { %>
        <a href="#" class="btn disabled btn-default btn-xs" title="{{ 'open_orchestra_backoffice.form.edit'|trans({}, 'messages', language) }}">
    <% } %>
            <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        </a>
    <% if (typeof links._self_delete !== 'undefined' ) { %>
        <a
            href="#" class="btn btn-danger btn-xs ajax-delete"
            title="{{ 'open_orchestra_backoffice.form.delete'|trans({}, 'messages', language) }}"
            data-title="{{ 'open_orchestra_backoffice.form.delete_confirm.title'|trans() }}"
            data-text="{{ 'open_orchestra_backoffice.form.delete_confirm.text'|trans() }}"
        >
    <% } else { %>
        <a href="#" class="btn disabled btn-default btn-xs" title="{{ 'open_orchestra_backoffice.form.delete'|trans({}, 'messages', language) }}">
    <% } %>
            <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
        </a>
<% } %>
