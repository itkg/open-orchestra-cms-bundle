{% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/Jarvis/header._tpl.twig' %}

<% if (typeof multiLanguage != 'undefined') { %>
    <ul id="entity-languages" class="nav nav-tabs">
    </ul>
<% } %>

<% if (typeof newVersion != 'undefined') { %>
    <span id="entity-new-version" data-text="{{ ('open_orchestra_backoffice.element.version.create') | trans({}, 'messages', language) }}"></span>
<% } %>

<% if (typeof multiVersion != 'undefined') { %>
    <div id="additionalContent" class="smart-form">
        <label class="select">
            <div id="version-selectbox"/>
            <i></i>
        </label>
    </div>
<% } %>

<div role="container">
    <%= html %>
    {% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/backToList._tpl.twig' %}
</div>

{% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/Jarvis/footer._tpl.twig' %}
