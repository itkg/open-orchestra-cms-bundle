<span rel="tooltip" data-placement="top" data-original-title="
    {{ 'open_orchestra_backoffice.element.last_update.at'| trans({}, 'messages', language) }}
    <%= element.get('updated_at') %>
    {{ 'open_orchestra_backoffice.element.last_update.by'| trans({}, 'messages', language) }}
    <% if (element.get('updated_by') !== undefined ) { %>
        <%= element.get('updated_by') %>
    <% } else { %>
        anonymous
    <% } %>
">
    <%= element.get('name') %>
    <% if (element.get('version')) { %>
        ({{ 'open_orchestra_backoffice.element.version.display' | trans({}, 'messages', language) }} : #<%= element.get('version') %>,
         {{ 'open_orchestra_backoffice.element.version.updated' | trans({}, 'messages', language) }} : <%= element.get('updated_at') %>)
    <% } %>
</span>
